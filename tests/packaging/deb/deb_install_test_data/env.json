{
    "dirs_config": {
        "cluster_manager": {
            "input_dir": "_build/default/rel/cluster_manager",
            "target_dir": "_build/default/rel/test_cluster"
        },
        "oz_worker": {
            "input_dir": "_build/default/rel/oz_worker",
            "target_dir": "_build/default/rel/test_cluster"
        }
    },
    "provider_domains": {},
    "zone_domains": {
        "oz1": {
            "db_driver": "couchdb",
            "cluster_manager": {
                "cm": {
                    "vm.args": {
                        "setcookie": "oz_cookie"
                    },
                    "sys.config": {
                        "cluster_manager": {
                            "cm_nodes": [
                                "cm"
                            ],
                            "worker_num": 1
                        }
                    }
                }
            },
            "oz_worker": {
                "node1": {
                    "vm.args": {
                        "setcookie": "oz_cookie"
                    },
                    "sys.config": {
                        "oz_worker": {
                            "cm_nodes": [
                                "cm"
                            ],
                            "db_nodes": [
                                "127.0.0.1:49161"
                            ],
                            "http_domain": {
                                "string": "127.0.0.1"
                            }
                        }
                    }
                }
            }
        }
    },
    "global_setup": {
        "users": {
            "provideradmin": {"default_space": "space1"}
        },
        "spaces": {
            "space1": {
                "displayed_name": "space1",
                "users": [
                    "provideradmin"
                ],
                "groups": [
                ],
                "providers": {
                }
            }
        }

    }
}
